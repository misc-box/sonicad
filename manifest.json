{
    "manifest_version": 2,
    "name": "Sonic Ad",
    "version": "1.0",
    "description": "Watch Ads really fast on Youtube",
    "permissions": ["activeTab", "storage"],
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["content.js"]
        }
    ],
    "browser_action": {
        "default_popup": "popup.html"
    }
}
